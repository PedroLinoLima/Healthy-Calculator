<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projecteixo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProjectEixo">
        <activity android:name=".ui.ResultadoNcdActivity" android:parentActivityName=".ui.NcdActivity"/>
        <activity android:name=".ui.ResultadoImcActivity" android:parentActivityName=".ui.ImcActivity"/>
        <activity android:name=".ui.NcdActivity" android:parentActivityName=".ui.DashBoardActivity"/>
        <activity android:name=".ui.ImcActivity" android:parentActivityName=".ui.DashBoardActivity"/>
        <activity android:name=".ui.DashBoardActivity" />
        <activity
            android:name=".ui.SplashScreen"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.Cadastro" />
        <activity android:name=".ui.Login" android:parentActivityName=".ui.MainActivity"/>
        <activity android:name=".ui.MainActivity"/>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>